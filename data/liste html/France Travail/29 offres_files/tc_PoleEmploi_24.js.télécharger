// *** [MOPINION] Variables JS Upload - Collecte ***
(function () {
const COMPONENT_SELECTOR = 'app-upload-semi-guide';
const EXPECTED_TEXT = 'Ajoutez un seul document complet pour chaque document attendu.';
const COOKIE_NOUVEL_UPLOAD = 'fusion_tool_nouvel_upload';
const COOKIE_CLICKED = 'fusion_tool_clicked';
const PATHNAME = "/fusiondepages"
const setCookieFT = function (name, value) {
document.cookie = name + '=' + encodeURIComponent(value) + ';path=/;SameSite=Lax';
};
const hasNouvelUploadGuide = function () {
const components = document.querySelectorAll(COMPONENT_SELECTOR);
for (const c of components) {
const b = c.querySelector('p > b');
if (b && b.textContent.trim() === EXPECTED_TEXT) {
return true;
}
}
return false;
};
const initIfReady = function (observer) {
if (!hasNouvelUploadGuide()) return false;
setCookieFT(COOKIE_NOUVEL_UPLOAD, '1');
document.addEventListener('click', function (e) {
const link = e.target.closest('a');
if (!link || !link.href) return;
try {
const pathname = new URL(link.href, location.origin).pathname;
if (pathname === PATHNAME) {
setCookieFT(COOKIE_CLICKED, '1');
}
} catch (_) {}
}, { once: true, capture: true });
if (observer) observer.disconnect();
return true;
};
const start = function () {
if (initIfReady(null)) return;
const observer = new MutationObserver(function () {
initIfReady(observer);
});
observer.observe(document.documentElement, {
childList: true,
subtree: true
});
};
start();
})();
}
});
tC.extend({
executeTag840_4340_24: function (el, p){
if (!el||!el.target){el={type:"no_event",target:{}}};
if(typeof p=="undefined"){p={};}
tc_array_events = tC.container_4340_24.init_tc_array_events(p);
var cact = tC.container_4340_24.cact || window.cact;
var cact_container = window.tC_4340_24;
var cact_event = el || {};
var cact_event_vars = Object.assign({}, tc_array_events);
var cact_event_attrs = cact_event.target || {};
// *** [MOPINION] Variable TMF - Icone ***
(function () {
'use strict';
var ab_cookie = 'ab_tmf_icon';
var original = 'original';
var variant = 'variant';
var ab_ratio = 0.5;
var ab_ttl_days = 30;
var MV_TEST = 'ab_tmf_icon';
var MV_CREATION = 'tmf_favori_icon';
var MV_WAVE = 'wave1';
if (window.__ABTEST_PATCH_OBS__) return;
window.__ABTEST_PATCH_OBS__ = true;
function tcGet(name) {
try {
if (window.tC && typeof window.tC.getCookie === 'function') return window.tC.getCookie(name);
return null;
} catch (_e) { return null; }
}
function tcSet(name, value, days) {
try {
if (window.tC && typeof window.tC.setCookie === 'function') {
window.tC.setCookie(name, value, days);
return true;
}
return false;
} catch (_e) { return false; }
}
function getOrAssignPopulation() {
var v = tcGet(ab_cookie);
if (v !== original && v !== variant) {
v = (Math.random() < ab_ratio) ? variant : original;
tcSet(ab_cookie, v, ab_ttl_days);
}
return v;
}
var pop = getOrAssignPopulation();
function sendMvDisplayOnce() {
if (window.__tmf_mv_display_sent__) return;
window.__tmf_mv_display_sent__ = true;
try {
if (window.pa && typeof window.pa.sendEvent === 'function') {
window.pa.sendEvent('mv_test.display', {
mv_creation: MV_CREATION,
mv_test: MV_TEST,
mv_wave: MV_WAVE,
mv_variant: pop
});
}
} catch (_e) {}
}
sendMvDisplayOnce();
//  Variant uniquement : CSS ab-faded + neutralisation fonction native 
if (pop === variant) {
delete window.mop_tmf_clic_favori;
(function injectAbCss() {
if (document.getElementById('ab-tmf-icon-style')) return;
var style = document.createElement('style');
style.id = 'ab-tmf-icon-style';
style.type = 'text/css';
style.textContent =
'.result-list .ab-faded{' +
'opacity:0.5;' +
'transition:opacity 200ms ease;' +
'}';
(document.head || document.getElementsByTagName('head')[0] || document.documentElement).appendChild(style);
})();
}
var PAGE_DIV_PREFIX = 'page_';
var LIST_SELECTOR = 'ul.result-list';
var LI_SELECTOR = 'li.result';
var MEDIA_SELECTOR = 'div.media-left';
var LINK_SELECTOR = 'a';
var ICON_SELECTOR = 'span.icon-formation';
var AB_DATA_ATTR = 'data-abtest';
var AB_DATA_VAL = 'true';
var AB_CLASS = 'abtest-link';
var ACTIVE_CLASS = 'active';
function ensureTcVars() {
if (!window.tc_vars || typeof window.tc_vars !== 'object') window.tc_vars = {};
return window.tc_vars;
}
function getPageIndexFromNode(node) {
try {
var n = node;
while (n && n !== document.documentElement) {
if (n.id && n.id.indexOf(PAGE_DIV_PREFIX) === 0) {
var s = n.id.slice(PAGE_DIV_PREFIX.length);
var i = parseInt(s, 10);
return isNaN(i) ? null : i;
}
n = n.parentNode;
}
return null;
} catch (_e) {
return null;
}
}
function getLocalIndexInUl(li) {
try {
var ul = li && li.closest ? li.closest(LIST_SELECTOR) : null;
if (!ul) return null;
var items = ul.querySelectorAll(LI_SELECTOR);
for (var i = 0; i < items.length; i++) {
if (items[i] === li) return i;
}
return null;
} catch (_e) {
return null;
}
}
function countResultsInPage(pageIndex) {
try {
var page = document.getElementById(PAGE_DIV_PREFIX + pageIndex);
if (!page) return 0;
var ul = page.querySelector(LIST_SELECTOR);
if (!ul) return 0;
return ul.querySelectorAll(LI_SELECTOR).length;
} catch (_e) {
return 0;
}
}
function getGlobalPosition(li) {
var pageIndex = getPageIndexFromNode(li);
var localIndex = getLocalIndexInUl(li);
if (pageIndex == null || localIndex == null) return null;
var offset = 0;
for (var p = 0; p < pageIndex; p++) offset += countResultsInPage(p);
return offset + localIndex + 1;
}
function setTcVarsForClick(displayType, globalPosition) {
var v = ensureTcVars();
v.type = 'action';
v.name = 'clic_favori';
v.chapters = [ab_cookie, displayType, String(globalPosition != null ? globalPosition : '')];
}
function fireClickTag() {
try {
if (typeof window.tCTag === 'function') window.tCTag();
} catch (_e) {}
}
function patchItem(li, index) {
var media = li.querySelector(MEDIA_SELECTOR);
if (!media) return;
var link = media.querySelector(LINK_SELECTOR);
if (link) {
link.id = 'abtest_' + (index + 1);
link.setAttribute(AB_DATA_ATTR, AB_DATA_VAL);
link.classList.add(AB_CLASS);
// href modifi UNIQUEMENT pour la variante
if (pop === variant) {
if (link.getAttribute('href') !== '#') link.setAttribute('href', '#');
}
}
// Swap icne UNIQUEMENT pour la variante
if (pop === variant) {
var icon = media.querySelector(ICON_SELECTOR);
if (icon) {
icon.classList.remove('icon-formation');
icon.classList.add('icon-star');
}
}
}
function patchUl(ul) {
if (!ul || ul.nodeType !== 1) return;
var items = ul.querySelectorAll(LI_SELECTOR);
for (var i = 0; i < items.length; i++) patchItem(items[i], i);
}
function patchExisting() {
var uls = document.querySelectorAll(LIST_SELECTOR);
for (var i = 0; i < uls.length; i++) patchUl(uls[i]);
}
function neutralizeActive(li) {
if (!li) return;
if (li.classList.contains(ACTIVE_CLASS)) {
li.classList.remove(ACTIVE_CLASS);
setTimeout(function () { li.classList.remove(ACTIVE_CLASS); }, 0);
}
}
function findLink(e) {
var sel = 'a.' + AB_CLASS + '[' + AB_DATA_ATTR + '="' + AB_DATA_VAL + '"]';
if (e.target && e.target.closest) {
var a = e.target.closest(sel);
if (a) return a;
}
if (e.composedPath) {
var p = e.composedPath();
for (var i = 0; i < p.length; i++) {
var n = p[i];
if (n && n.nodeType === 1 && n.matches && n.matches(sel)) return n;
}
}
return null;
}
//  Les deux populations : patch attributs de tracking 
patchExisting();
//  Les deux populations : tracking clic 
document.addEventListener('pointerdown', function (e) {
var a = findLink(e);
if (!a) return;
var li = a.closest(LI_SELECTOR);
if (!li) return;
var mediaLeft = li.querySelector(MEDIA_SELECTOR);
if (!mediaLeft) return;
// Tout ce qui est variant-only : bloquer le clic natif + feedback visuel
if (pop === variant) {
e.preventDefault();
e.stopPropagation();
if (e.stopImmediatePropagation) e.stopImmediatePropagation();
if (a.blur) a.blur();
window.mop_tmf_clic_favori = true; // neutralise la fonction native ct variant
mediaLeft.classList.add('ab-faded');
neutralizeActive(li);
}
// Tracking commun aux deux populations
var pos = getGlobalPosition(li);
setTcVarsForClick(pop, pos);
fireClickTag();
}, true);
//  Observer : mutations DOM (les deux populations) 
var observer = new MutationObserver(function (mutations) {
for (var i = 0; i < mutations.length; i++) {
var m = mutations[i];
if (m.type === 'attributes' && m.attributeName === 'class') {
if (pop === variant) {
var t = m.target;
if (t && t.matches && t.matches(LI_SELECTOR)) neutralizeActive(t);
}
continue;
}
if (m.type !== 'childList') continue;
for (var j = 0; j < m.addedNodes.length; j++) {
var n = m.addedNodes[j];
if (!n || n.nodeType !== 1) continue;
if (n.matches && n.matches(LIST_SELECTOR)) patchUl(n);
else if (n.querySelectorAll) {
var uls = n.querySelectorAll(LIST_SELECTOR);
for (var k = 0; k < uls.length; k++) patchUl(uls[k]);
}
}
}
});
if (document.body) {
observer.observe(document.body, {
subtree: true,
childList: true,
attributes: true,
attributeFilter: ['class']
});
}
})();
}
});
tC.extend({
executeTag834_4340_24: function (el, p){
if (!el||!el.target){el={type:"no_event",target:{}}};
if(typeof p=="undefined"){p={};}
tc_array_events = tC.container_4340_24.init_tc_array_events(p);
var cact = tC.container_4340_24.cact || window.cact;
var cact_container = window.tC_4340_24;
var cact_event = el || {};
var cact_event_vars = Object.assign({}, tc_array_events);
var cact_event_attrs = cact_event.target || {};
// *** [MOPINION] Variables TMF ***
(function () {
// viter les doublons
if (window._modalMopinionObserver) return;
'use strict';
const MODAL_SEL = '#PopinDetails';
const ZONE_SEL  = '#zonePrendreRDV';
let modal = null;
let wasVisible = false;
let pollIntervalId = null;
let lastNameSeen = null;
let tcNameAtOpen = null;
let hasTcNameChanged = false;
let titleAtOpen = null;
// NEW: garder la rfrence du noeud rellement observ
let observedNode = null;
function $(sel) { return document.querySelector(sel); }
function isVisible(el) {
if (!el || !el.isConnected) return false;
const cs = getComputedStyle(el);
if (cs.display === 'none' || cs.visibility === 'hidden' || cs.opacity === '0') return false;
const r = el.getBoundingClientRect();
return r.width > 0 && r.height > 0;
}
function getModal() {
if (!modal || !document.body.contains(modal)) modal = $(MODAL_SEL) || null;
return modal;
}
function getZone() { return $(ZONE_SEL); }
function setModalite(newValue) {
if (window._internal_mopinion_tmf_modalite) return;
window._internal_mopinion_tmf_modalite = newValue;
}
function startPolling() {
if (pollIntervalId !== null) return;
pollIntervalId = window.setInterval(function () {
try {
const m = getModal();
if (!m || !isVisible(m)) {
stopPolling();
return;
}
const zone = getZone();
const tc   = window.tc_vars;
const name = tc && tc.name ? String(tc.name) : null;
if (!hasTcNameChanged &&
((tcNameAtOpen === null && name !== null) ||
(name !== null && name !== tcNameAtOpen))
) {
hasTcNameChanged = true;
}
// --- Logique FOAD base sur le titre, tant que tc_vars.name n'a pas chang ---
if (!hasTcNameChanged && zone) {
const rdvDiv = zone.querySelector('#rdv');
const h2 = rdvDiv && rdvDiv.querySelector('h2');
const titre = h2 && h2.innerText ? h2.innerText.trim() : '';
const isFOAD = window._internal_mopinion_tmf_modalite === 'FOAD';
const titreInitial = titleAtOpen || '';
if (isFOAD) {
if (titre !== titreInitial && titre !== 'Inscription') {
resetInternalVars();
titleAtOpen = titre;
}
} else {
if (titre !== titreInitial && titre === 'Inscription') {
const blockFoad = zone.querySelector('#blockFoadZone');
if (blockFoad) {
const h3 = blockFoad.querySelector('h3');
const sousTitre = h3 && h3.innerText ? h3.innerText.trim() : '';
if (sousTitre === 'Pr-requis ncessaire  cette formation') {
window._internal_mopinion_tmf_modalite = 'FOAD';
}
}
titleAtOpen = titre;
}
}
}
if (!name) return;
if (name !== lastNameSeen) lastNameSeen = name;
if (name === 'Preinscription_FOAD') {
window._internal_mopinion_tmf_complete = true;
stopPolling();
} else if (name === 'Choix_date_RDV') {
window._internal_mopinion_tmf_step = 2;
let choixZone = document.querySelector('#choixRendezVousFormZone');
if (!choixZone && zone) choixZone = zone.querySelector('#choixRendezVousFormZone');
if (!choixZone) return;
const h3 = choixZone.querySelector('h3');
const label = h3 && h3.innerText ? h3.innerText.trim() : '';
if (label === 'Choisir votre rendez-vous dinformation :') setModalite('RDV');
else if (label === 'Aucun rendez-vous nest disponible.') setModalite('PSI');
} else if (name === 'Recapitulatif') {
window._internal_mopinion_tmf_step = 3;
window._internal_mopinion_tmf_step3 = true;
if (zone) {
const sessionDiv = zone.querySelector('div.session');
sessionDiv ? setModalite('PSI') : setModalite('RDV');
}
} else if (name === 'Confirmation_RDV_OK') {
window._internal_mopinion_tmf_step = null;
window._internal_mopinion_tmf_step3 = null;
window._internal_mopinion_tmf_complete = true;
}
} catch (e) {
tC.log('[Modale TMF] ERREUR dans le poller tc_vars.name :', e);
}
}, 500);
}
function stopPolling() {
if (pollIntervalId !== null) {
window.clearInterval(pollIntervalId);
pollIntervalId = null;
}
}
function resetInternalVars() {
window._internal_mopinion_tmf_step = null;
window._internal_mopinion_tmf_step3 = false;
window._internal_mopinion_tmf_complete = false;
window._internal_mopinion_tmf_modalite = null;
}
function resetExposedVars() {
window.mopinion_tmf_step = null;
window.mopinion_tmf_step3 = false;
window.mopinion_tmf_complete = false;
window.mopinion_tmf_modalite = null;
}
function onModalOpen() {
resetInternalVars();
resetExposedVars();
lastNameSeen = null;
const tc = window.tc_vars;
tcNameAtOpen = tc && tc.name ? String(tc.name) : null;
hasTcNameChanged = false;
const zone = getZone();
if (zone) {
const rdvDiv = zone.querySelector('#rdv');
const h2 = rdvDiv && rdvDiv.querySelector('h2');
titleAtOpen = h2 && h2.innerText ? h2.innerText.trim() : '';
} else {
titleAtOpen = '';
}
startPolling();
}
function onModalClose() {
stopPolling();
const m = window._internal_mopinion_tmf_modalite;
if (!m) {
resetInternalVars();
return;
}
window.mopinion_tmf_modalite = m;
window.mopinion_tmf_complete = window._internal_mopinion_tmf_complete;
if (m !== 'FOAD') {
window.mopinion_tmf_step = window._internal_mopinion_tmf_step3 ? 3 : window._internal_mopinion_tmf_step;
}
resetInternalVars();
}
// NEW: force un check dtat mme sans mutation (modale dj ouverte)
function ensureState() {
const m = getModal();
const visible = m ? isVisible(m) : false;
if (visible && !wasVisible) onModalOpen();
else if (!visible && wasVisible) onModalClose();
wasVisible = visible;
}
const observer = new MutationObserver(function () {
// NEW: si la modale a t remplace, on r-attache
const m = getModal();
if (m && observedNode && m !== observedNode) {
try { observer.disconnect(); } catch (_e) {}
observedNode = null;
startObserving(); // r-attache
return;
}
ensureState();
});
function startObserving() {
const m = getModal();
// NEW: si on observe dj le bon noeud, on ne refait rien
if (m && observedNode === m) {
ensureState();
return;
}
try { observer.disconnect(); } catch (_e) {}
observedNode = null;
if (m) {
observedNode = m;
observer.observe(m, {
attributes: true,
attributeFilter: ['style', 'class', 'hidden', 'aria-hidden'],
attributeOldValue: true
});
} else if (document.body) {
observedNode = document.body;
observer.observe(document.body, {
childList: true,
subtree: true,
attributes: true,
attributeFilter: ['style', 'class', 'hidden', 'aria-hidden']
});
}
// NEW: check initial immdiat (cas retour dauth avec modale dj ouverte)
ensureState();
}
window._modalMopinionObserver = {
instance: observer,
stop: function () {
stopPolling();
observer.disconnect();
delete window._modalMopinionObserver;
tC.log('[Modale TMF] Observer arrt et supprim');
},
isActive: function () { return !!this.instance; }
};
// NEW: au retour navigation (bfcache / pageshow), on re-check
window.addEventListener('pageshow', function () {
startObserving();
});
if (document.readyState === 'loading') {
document.addEventListener('DOMContentLoaded', startObserving);
} else {
startObserving();
}
})();
}
});
