quoi+" - "+ou;description="Retrouvez toutes les offres d'emploi pour "+quoi+" - "+ou+" et postulez tout de suite"}else if(quoi&&!ou){titre+=quoi;description="Retrouvez toutes les offres d'emploi pour "+quoi+" et postulez tout de suite"}else if(!quoi&&ou){titre+=ou;description="Retrouvez toutes les offres d'emploi pour "+ou+" et postulez tout de suite"}else titre="Offres d'emploi";titre+=" | France Travail";document.title=titre;$("meta[name\x3ddescription]")[0].content=description}function rechargerTitreCalcule(idElement,
title){var titre=title;var description="Retrouvez toutes les offres d'emploi pour "+title+" et postulez tout de suite";titre+=" | France Travail";document.title=titre;$("meta[name\x3ddescription]")[0].content=description}function rechargerTitreSelection(nbOffres){var titre="Ma sélection d'offres - ";if(nbOffres>0)if(nbOffres>1)titre+=nbOffres+" offres sélectionnées";else titre+=nbOffres+" offre sélectionnée";else titre+="aucune offre sélectionnée";titre+=" | France Travail";document.title=titre}})}).call(this);
