function connect(event){var form=document.createElement("form");form.method="POST";form.action=connectUrl;var input=document.createElement("input");input.type="hidden";input.name="url";input.value=url;form.appendChild(input);document.body.appendChild(form);form.submit()}document.addEventListener("ft-header-deconnect",connect);
customElements.whenDefined("ft-header").then(function(){document.querySelector("body \x3e ft-header").shadowRoot.querySelectorAll('a[href*\x3d"espacepersonnel"]:not([data-listener-attached])').forEach(function(link){link.addEventListener("click",function(event){event.preventDefault();connect(event)});link.setAttribute("data-listener-attached","true")})});